{
  "kind": "collectionType",
  "collectionName": "address",
  "info": {
    "name": "address"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "addresses"
    },
    "streetName": {
      "type": "text"
    },
    "suite": {
      "type": "text"
    },
    "city": {
      "type": "string"
    },
    "country": {
      "type": "string"
    },
    "addressName": {
      "type": "string",
      "required": true
    }
  }
}
